{
    "name": "tanthammar/livewire-auto-routes",
    "description": "Auto generate routes for Laravel Livewire Components.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "livewire",
        "auto-routes"
    ],
    "authors": [
        {
            "name": "Tina Hammar",
            "email": "tinahammar@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0",
        "livewire/livewire": "^2.4|dev-master"
    },
    "autoload": {
        "psr-4": {
            "Tanthammar\\LivewireAutoRoutes\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tanthammar\\LivewireAutoRoutes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tanthammar\\LivewireAutoRoutes\\LivewireAutoRoutesServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
